{
    "name": "{name}",
    "version": "0.0.0",
    "package_version": 2,
    "type": "module",
    "main": "./dist/browser.cjs",
    "scripts": {
        "dev": "npm run build -- --mode development",
        "build": "vue-tsc -b && vite build"
    },
    "dependencies": {
        "element-plus": "^2.8.6",
        "vue": "^3.4.37"
    },
    "devDependencies": {
        "@cocos-fe/vite-plugin-cocos-panel": "^0.0.1-beta.9",
        "@cocos/creator-types": "^3.8.6",
        "@types/node": "^22.15.21",
        "@vitejs/plugin-vue": "^5.2.2",
        "@vue/tsconfig": "^0.7.0",
        "rollup-plugin-node-externals": "^7.1.3",
        "typescript": "^5.8.2",
        "unplugin-auto-import": "^0.18.6",
        "unplugin-vue-components": "^0.27.5",
        "vite": "^5.4.1",
        "vue-tsc": "^2.1.10"
    },
    "contributions": {
        "menu": [
            {
                "path": "i18n:menu.panel/{name}",
                "label": "i18n:{name}.title",
                "message": "open"
            },
            {
                "path": "i18n:menu.panel/{name}",
                "label": "send to panel",
                "message": "increase"
            }
        ],
        "messages": {
            "open": {
                "methods": [
                    "open"
                ]
            },
            "get-version": {
                "methods": [
                    "getVersion"
                ]
            },
            "increase": {
                "methods": [
                    "default.increase"
                ]
            }
        }
    },
    "panels": {
        "default": {
            "main": "./dist/panel.cjs",
            "size": {
                "min-width": 400,
                "min-height": 300,
                "width": 800,
                "height": 700
            }
        }
    }
}